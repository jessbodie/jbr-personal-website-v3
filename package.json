{
  "name": "jbr-website",
  "version": "3.0.0",
  "description": "Jess Bodie Richards - Personal Website v 3.0",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessbodie/jbr-personal-website-v3"
  },
  "scripts": {
    "compile:sass": "sass public/sass/main.scss public/css/style.css",
    "prefix:css": "postcss public/css/style.css -o public/css/style.prefix.css",
    "compress:css": "sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass",
    "_alternate2_build:css": "npm-run-all compile:sass compress:css",
    "_alternate_build:css": "npm-run-all compile:sass prefix:css compress:css",
    "watch": "nodemon -e js,hbs",
    "watch:sass": "nodemon -e scss -x \"npm run compile:sass\"",
    "start:dev": "npm-run-all -p watch:sass watch",
    "start:node": "node index.js",
    "start": "npm-run-all build:css start:node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jess Bodie Richards",
  "license": "ISC",
  "homepage": "https://github.com/jessbodie/",
  "dependencies": {
    "express": "^4.18.2",
    "hbs": "^4.0.1",
    "html-webpack-plugin": "^5.6.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@webpack-cli/generators": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concat": "^1.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "handlebars-template-loader": "^0.1.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "sass": "^1.84.0",
    "sass-loader": "^13.3.2",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.1.4"
  }
}
